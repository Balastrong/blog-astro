---
import ProjectsGrid from '~/components/widgets/Projects.astro';
import { myProjects } from '~/data';
import Layout from '~/layouts/PageLayout.astro';

const meta = {
  title: 'My Open Source projects',
};
---

<Layout {meta}>
  <ProjectsGrid
    title="My Projects"
    subtitle="Some of my favourite Open Source projects I own/maintain"
    highlight="Open Source"
    repositories={myProjects}
  />
</Layout>
